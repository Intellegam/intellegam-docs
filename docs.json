{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "name": "Intellegam Docs",
  "colors": {
    "primary": "#7F74F0",
    "light": "#7F74F0",
    "dark": "#7F74F0"
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "IFrame integration",
            "pages": [
              "/IFrame/overview",
              {
                "group": "Control the IFrame",
                "pages": ["/IFrame/host-control-api", "/IFrame/action-types"]
              },
              "/IFrame/query-params"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": ["api-reference/introduction"]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/light_with_border.svg",
    "dark": "/logo/dark_with_border.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@intellegam.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {}
}
